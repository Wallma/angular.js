<p id="notice"><%= notice %></p>

<h1>Listing Resellers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resellers.each do |reseller| %>
      <tr>
        <td><%= reseller.name %></td>
        <td><%= reseller.image %></td>
        <td><%= link_to 'Show', reseller %></td>
        <td><%= link_to 'Edit', edit_reseller_path(reseller) %></td>
        <td><%= link_to 'Destroy', reseller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reseller', new_reseller_path %>
